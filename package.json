{
  "name": "crab.so",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\" --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.1",
    "prettier": "latest",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.1.0",
  "pnpm": {
    "patchedDependencies": {
      "@spling/social-protocol@1.5.6": "patches/@spling__social-protocol@1.5.6.patch"
    }
  }
}
